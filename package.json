    "graphql": "^16.8.1",
    "houdini": "^1.3.1",
    "houdini-svelte": "^2.0.1",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^5.25.3",
